{
  "name": ".",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start --tunnel --clear --reset-cache",
    "start:dev": "expo start --tunnel --clear --reset-cache",
    "start:prod": "APP_ENV=prod EXPO_PUBLIC_APP_ENV=prod NODE_ENV=production expo start --tunnel --clear --reset-cache",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "api:dev": "tsx watch api/index.ts",
    "api:start": "NODE_ENV=production tsx api/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "test": "jest",
    "coverage": "jest --coverage && rimraf coverage/",
    "reset:deps": "rm -rf node_modules .expo .pnpm-store && pnpm install",
    "lines": "git ls-files \"\": ':(exclude)*.json' \"\": ':(exclude)*.md' \"\": ':(exclude)*.yaml' \"\": ':(exclude)*.yml' | xargs wc -l"
  },
